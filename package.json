{
  "name": "@dhruv-m-patel/react-autocomplete",
  "version": "1.0.3",
  "description": "An autocomplete component built with react",
  "author": "Dhruv Patel <https://github.com/dhruv-m-patel>",
  "homepage": "https://github.com/dhruv-m-patel/react-autocomplete",
  "keywords": [
    "React",
    "Autocomplete"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dhruv-m-patel/react-autocomplete.git"
  },
  "bugs": {
    "url": "https://github.com/dhruv-m-patel/react-autocomplete/issues"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build:demo": "cd demo && npm run build && cd ..",
    "build": "BABEL_ENV=production babel src --out-dir dist --copy-files && npm run build:demo",
    "start": "cd demo && node ./dist/demo/app/server.js",
    "pretest": "jest --clearCache",
    "test": "jest",
    "ci:test": "jest --updateSnapshot --coverage",
    "publish:component": "BABEL_ENV=production npm run build && npm publish --access public",
    "postinstall": "cd demo && npm install && cd .."
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-macros": "^2.6.1",
    "babel-preset-minify": "^0.5.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "^3.0.8",
    "jest": "24.9.0",
    "jest-enzyme": "^7.1.1",
    "jsdom": "^15.1.1",
    "postcss-cssnext": "^3.1.0",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.11.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
