  "scripts": {
    "clean": "rimraf dist && rimraf dist-demo",
    "build:demo": "babel ./src --out-dir dist-demo/src && babel ./demo --out-dir dist-demo/demo && webpack --config=webpack-demo.config.js",
    "build:component": "NODE_ENV=production webpack --config=webpack.config.js",
    "build": "npm run build:demo && npm run build:component",
    "start": "node ./dist-demo/demo/server.js",
    "lint:js": "eslint src/**/*.js && eslint src/**/*.jsx && eslint demo/**/*.js && eslint demo/**/*.jsx",
    "lint:css": "stylelint \"./src/**/*.css\"",
    "lint:fix": "eslint --fix src/**/*.js && eslint --fix src/**/*.jsx && eslint --fix demo/**/*.js && eslint --fix demo/**/*.jsx",
    "lint": "npm run lint:js && npm run lint:css",
    "pretest": "jest --clearCache",
    "test": "jest",
    "ci:test": "jest --updateSnapshot --coverage",
    "publish:component": "NODE_ENV=production npm run build && npm publish --access public"
  },
  "bugs": {
    "url": "https://github.com/dhruv-m-patel/react-autocomplete/issues"
  },
  "directories": {
    "test": "tests"
  }
